<?xml version="1.0" encoding="UTF-8"?>

<templates>
	<template id="template-main-waysPanelHeader">
	<![CDATA[
<div class="result_numb" id="result_numb_<%=index%>"
			onclick="on_selectDetailWay(<%=index%>)">
			<div id="rout_numb">

				<div class="numb">
					<a id="way_ref_<%=index%>"
						style="color: white; font-size: 24px; font-weight: 900;"
						onclick="on_selectWay(<%=index%>)">
						<%=index%>

					</a>
				</div>


			</div>
			<div id="cost_info">
				<p><%= locale("cost") + cost  + " " + locale("UAH") %></p>
				<p><%= locale("time") + time%></p>
			</div>
		</div>
<div id="res_text" style="width: 322px; margin-left: 10px; white-space: normal; "> 
<div id="print">
				<a href="#"><img src="media/cityways/themes/default/images/print.png"
					title="Print"></a>

			</div>
			<div id="clipboard">
				<a href="#"><img src="media/cityways/themes/default/images/chain.png"
					title="Clipboard"></a>
			</div>
				<p><%= locale("byFootToStation") + footTo%></p>
				<p><%= locale("timeFoot") %></p>
				<p><%= locale("distanceFoot")  %></p>
				<p><%= locale("cost")  %></p>
				<p><%= locale("interval")  %></p>
				<p><%= locale("time") %></p>
				<p><%= locale("distanceTransport") %></p>
				<p><%= locale("getIn")  %></p>
				<p><%= locale("getOut") %></p>
				<p><%= locale("transfer") %></p>
				<p><%= locale("transferFrom") %></p>
				<p><%= locale("transferTo") %></p>
</div>
	]]>
	</template>
	<template id="template-main-waysPanelInfo"> <![CDATA[
		<p><%= value %></p>
		<p><%= locale("path_time") %></p>

		<div class="start"> 
			<div class="userplace">
				<input type="text" id="username" />
			</div>
			<div class="buttonplace">
				<input type="button" value="Проверить" />
			</div>
		</div>
	]]>
	</template>
	

	
	<template id="template-main-product"><![CDATA[
		<div>
			<p>sdfdsf</p>

		</div>
	]]>
	</template>
</templates>